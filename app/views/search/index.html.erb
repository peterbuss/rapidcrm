<h1>Search Results</h1>

<div class="container">
	<div class="row clearfix">
		<div class="col-md-12 column">
			<table class="table">
				<thead>
					<tr>
						<th>
							Name
						</th>
						<th>
							Company
						</th>
						<th>
						 Location
						</th>
						<th>
              Phone
						</th>
            <th>
            </th>
					</tr>
				</thead>
				<tbody>
          <% @leads.each do |lead| %>
					<tr>
						<td>
							<%= lead.name %>
						</td>
						<td>
							 <%= lead.company %>
						</td>
						<td>
							<%= lead.location %>
						</td>
						<td>
							<%= lead.phone %>
						</td>
            <td>
              <%= link_to 'Edit', edit_lead_path(lead) %>
            </td>
					</tr>
          <% end %>
				</tbody>
			</table>
		</div>
	</div>
</div>

